	devfreq-cpufreq {
		cpubw-cpufreq {
		target-dev = <&cpubw>;
		cpu-to-dev-map =
			 < 652800 2124>,
			 < 806400 2124>,
			 <1036800 3221>,
			 <1209600 5126>,
			 <1401600 5859>,
			 <1689600 6445>,
			 <1804800 7104>,
			 <2016000 7104>,
			 <2208000 7104>;
			};

		mincpubw-cpufreq {
			target-dev = <&mincpubw>;
			cpu-to-dev-map =
				<  652800 2124 >,
				< 1401600 3221 >,
				< 2208000 5859 >;
			};
		};

	qcom,msm-thermal {
		msm_thermal_freq: qcom,vdd-apps-rstr {
			qcom,vdd-rstr-reg = "vdd-apps";
			qcom,levels = <652800 2016000 2208000>;
			qcom,freq-req;
		};
	};

	msm_cpufreq: qcom,msm-cpufreq {
		qcom,cpufreq-table =
			 < 652800>,
			 < 806400>,
			 <1036800>,
			 <1209600>,
			 <1401600>,
			 <1689600>,
			 <1804800>,
			 <2016000>,
			 <2208000>;
	};

};

