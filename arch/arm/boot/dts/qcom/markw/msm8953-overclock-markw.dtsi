			    "c0-pll", "c0-mux", "c1-mux",
			    "cci-mux", "efuse";
		vdd-mx-supply = <&pm8953_s7_level_ao>;
		vdd-cl-supply = <&apc_vreg>;
		clocks = <&clock_gcc clk_xo_a_clk_src>;
		clock-names = "xo_a";
		qcom,num-clusters = <2>;
		qcom,speed0-bin-v0-cl =
			<          0 0>,
			<  652800000 1>,
			<  806400000 2>,
			< 1036800000 3>,
			< 1209600000 4>,
			< 1401600000 5>,
			< 1689600000 6>,
			< 1804800000 7>,
			< 2016000000 8>,
			< 2208000000 9>;
		qcom,speed0-bin-v0-cci =
			<          0 0>,
			<  261120000 1>,
			<  320000000 2>,
			<  414720000 3>,
			<  483840000 4>,
			<  560640000 5>,
			<  675840000 6>,
			<  721920000 7>,
			<  806400000 8>,
			<  883200000 9>;

		qcom,speed2-bin-v0-cl =
			<          0 0>,
			<  652800000 1>,
			<  806400000 2>,
			< 1036800000 3>,
			< 1209600000 4>,
			< 1401600000 5>,
			< 1689600000 6>,
			< 1804800000 7>,
			< 2016000000 8>,
			< 2208000000 9>;

		qcom,speed2-bin-v0-cci =
			<          0 0>,
			<  261120000 1>,
			<  320000000 2>,
			<  414720000 3>,
			<  483840000 4>,
			<  560640000 5>,
			<  675840000 6>,
			<  721920000 7>,
			<  806400000 8>,
			<  883200000 9>;
		qcom,speed7-bin-v0-cl =
			<          0 0>,
			<  652800000 1>,
			<  806400000 2>,
			< 1036800000 3>,
			< 1209600000 4>,
			< 1401600000 5>,
			< 1689600000 6>,
			< 1804800000 7>,
			< 2016000000 8>,
			< 2208000000 9>;
