			<          0 0>,
			<  261120000 1>,
			<  320000000 2>,
			<  414720000 3>,
			<  483840000 4>,
			<  560640000 5>,
			<  675840000 6>,
			<  721920000 7>,
			<  806400000 8>,
			<  883200000 9>;
